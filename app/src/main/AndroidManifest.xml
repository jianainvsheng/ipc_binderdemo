<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.report.com.gramnardemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".kottlin.KottlinDemoActivity"/>
        <activity android:name=".kottlin.KottlinNextDemoActivity" />
        <activity android:name=".aidl.AidlActivity"/>
        <service
            android:name=".aidl.service.PersionService"
            android:enabled="true"
            android:exported="true"
            android:process=":aidl" />
    </application>

</manifest>